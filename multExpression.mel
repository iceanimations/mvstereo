
float $mult = $s_mult;
float $hfo = $s_hfoAttr;
float $offset = $s_offsetAttr;
float $rot = $s_rotAttr;
float $distance = $s_zeroParallaxPlaneAttr;

if ($rot) $rot = rad_to_deg(atan(($mult * $offset)/$distance));
else $rot = 0;

$s_camTranslateXAttr = $mult * $offset;
$s_camHfoAttr = $mult * $hfo;
$s_camRotXAttr = $rot;

